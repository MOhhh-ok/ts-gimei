{
  "name": "ts-gimei",
  "version": "0.3.3",
  "type": "module",
  "description": "日本語の人物名や住所のテストデータを、漢字・ひらがな・カタカナ・ローマ字で出力します。ブラウザ・Node.js両方で使用可能",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": "https://github.com/mohhh-ok/ts-gimei",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "bun run generate && bun scripts/build.ts",
    "dev": "tsc --noEmit --watch",
    "type-check": "tsc --noEmit",
    "preinstall": "npx only-allow bun",
    "generate": "bun scripts/generate.ts",
    "test": "bun src/test.ts",
    "prepublishOnly": "bun run build"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/bun": "^1.3.5",
    "@types/fs-extra": "^11.0.4",
    "bun-plugin-dts": "^0.3.0",
    "fs-extra": "^11.3.3",
    "only-allow": "^1.2.2",
    "path": "^0.12.7",
    "release-it": "^19.2.2",
    "yaml": "^2.8.2"
  },
  "keywords": [
    "gimei",
    "japanese",
    "name",
    "address",
    "generator",
    "faker"
  ],
  "license": "MIT"
}
